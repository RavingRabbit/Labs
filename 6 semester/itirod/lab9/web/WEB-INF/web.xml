<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
		  http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
         version="3.0">
    <servlet>
        <servlet-name>clients</servlet-name>
        <jsp-file>/clients.jsp</jsp-file>
    </servlet>
    <servlet>
        <servlet-name>delete</servlet-name>
        <jsp-file>/client/delete.jsp</jsp-file>
    </servlet>
    <servlet>
        <servlet-name>edit</servlet-name>
        <jsp-file>/client/edit.jsp</jsp-file>
    </servlet>
    <servlet>
        <servlet-name>new</servlet-name>
        <jsp-file>/client/new.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>clients</servlet-name>
        <url-pattern>/clients</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>delete</servlet-name>
        <url-pattern>/client/delete</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>edit</servlet-name>
        <url-pattern>/client/edit</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>new</servlet-name>
        <url-pattern>/client/new</url-pattern>
    </servlet-mapping>
</web-app>
